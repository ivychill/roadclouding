%h2 Sign In
= form_for(resource, :as => resource_name, :url => sessions_path) do |f|
  = devise_error_messages!
  %div
    = f.label :email
    %br/
    = f.email_field :email
  %div
    = f.label :password
    %br/
    = f.password_field :password
  %div= f.submit "Sign In"
= render :partial => "devise/shared/links"